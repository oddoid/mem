{
  "name": "mem-font",
  "version": "3.1.0",
  "description": "4x4 monospaced and proportional pixel fonts",
  "keywords": [
    "font",
    "pixel",
    "monospaced",
    "fixed-width",
    "proportional",
    "8-bit",
    "retro",
    "game",
    "minimal",
    "code page 437"
  ],
  "homepage": "https://github.com/rndmem/mem-font",
  "repository": "github:rndmem/mem-font",
  "bugs": "https://github.com/rndmem/mem-font/issues",
  "scripts": {
    "build": "bin/make",
    "rebuild": "bin/make clean && bin/make",
    "clean": "bin/make clean",
    "preversion": "[ -z \"$(git status --porcelain)\" ] && npm run -s rebuild && git add build",
    "postversion": "git push origin master --follow-tags && npm publish",
    "clobber": "npm run -s clean && rm -rf node_modules"
  },
  "engines": {
    "node": "7.3.0"
  },
  "license": "GPL-3.0"
}
