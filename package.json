{
  "name": "mem-font",
  "version": "1.0.0",
  "description": "4x4 fixed width pixel font",
  "keywords": [
    "font",
    "pixel",
    "fixed width",
    "8-bit",
    "retro",
    "game",
    "minimal"
  ],
  "homepage": "https://github.com/rndmem/mem-font",
  "repository": "github:rndmem/mem-font",
  "bugs": "https://github.com/rndmem/mem-font/issues",
  "scripts": {
    "build": "bin/make",
    "release": "bin/make release",
    "preversion": "[ -z \"$(git status --porcelain)\" ]",
    "postversion": "git push origin master --follow-tags && npm publish",
    "clean": "bin/make clean",
    "clobber": "npm run -s clean && rm -rf node_modules"
  },
  "engines": {
    "node": "7.1.0"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "jq-cli-wrapper": "0.4.0"
  }
}
